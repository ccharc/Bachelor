MinimizeCG51 <- function(x_k, A, b, k_max = 1000, tolerance = sqrt(.Machine$double.eps)) {
# Given x_0;
r_k <- Ax_k - b; p_k <- -r_k; k <- 0
while(r_k != 0 & k < k_max){
alpha_k <- - (t(r_k) * p_k)/(t(p_k) %*% A %*% p_k);
x_k <- x_k + alpha_k * p_k;
r_k <- A %*% x_k - b;
beta_k <- (t(r_k) %*% A %*% p_k)/ (t(p_k) %*% A %*% p_k);
p_k <- - r_k + beta_k * p_k
k <- k+1
}
return(x_k)
}
